{
    "name": "danethomas/craft-cta-field",
    "description": "Call to action & link fields made easy",
    "type": "craft-plugin",
    "version": "3.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "cta"
    ],
    "support": {
        "docs": "https://github.com/danethomas/craft-ctafield/blob/master/README.md",
        "issues": "https://github.com/danethomas/craft-ctafield/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dane Thomas",
            "homepage": ""
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0|^5.0.0"
    },
    "autoload": {
        "psr-4": {
          "danethomas\\cta\\": "src/"
        }
    },
    "extra": {
        "name": "CTA",
        "handle": "cta",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/danethomas/craft-ctafield/master/CHANGELOG.md",
        "class": "danethomas\\cta\\CTA"
    }
}
